@{
    ViewBag.Title = "Weather";
    Layout = "~/Views/Shared/_Layout.cshtml";
} 

<div class="row">
        <div class="col-sm-1" style="color:white;">
                @Model.SunRise | 
        </div>
    <div class="col-sm-2">
        <img src="~/Images/cartoon-sun-vector-art2.png"/>
    </div>
        <div class="col-sm-6">
            <DIV >
                       
            </DIV>
        </div>
        <div class="col-sm-2" >
                <img src="~/Images/moon.png"/>
        </div>
    <div class="col-sm-1" style="color:white;">
        | @Model.SunSet
    </div>
</div>
<div class="row">
    <div class="col-md-4">

        <div class="text-center" style="color:white;">
            <h4>@Model.Time1Day</h4>
            @Model.Symbol1<br />
            Precipitation | @Model.Precipitation1<br />
            Wind Direction | @Model.WindDirection1<br />
            Wind speed | @Model.WindSpeed1<br />
            Temperature | @Model.Temperature1<br />
            Pressure | @Model.Pressure1<br />
            Humidity | @Model.Humidity1<br />
            Clouds | @Model.Cloud1
        </div>

    </div>

    <div class="col-md-4" style="color:white;">

        <div class="text-center">
            <h4>@Model.Time2Day</h4>
            @Model.Symbol2<br />
            Precipitation | @Model.Precipitation2<br />
            Wind Direction | @Model.WindDirection2<br />
            Wind speed | @Model.WindSpeed2<br />
            Temperature | @Model.Temperature2<br />
            Pressure | @Model.Pressure2<br />
            Humidity | @Model.Humidity2<br />
            Clouds | @Model.Cloud2
        </div>
    </div>

    <div class="col-md-4">

        <div class="text-center" style="color:white;">
            <h4>@Model.Time3Day</h4>
            @Model.Symbol3<br />
            Precipitation | @Model.Precipitation3<br />
            Wind Direction | @Model.WindDirection3<br />
            Wind speed | @Model.WindSpeed3<br />
            Temperature | @Model.Temperature3<br />
            Pressure | @Model.Pressure3<br />
            Humidity | @Model.Humidity3<br />
            Clouds | @Model.Cloud3
        </div>

    </div>

</div>
<div class="row">
    <div class="col-md-4">
            <img src=@Model.BackImg1 />
    </div>
    <div class="col-md-4">
            <img src=@Model.BackImg2 />
    </div>
    <div class="col-md-4">
            <img src=@Model.BackImg3 />
    </div>
</div>






